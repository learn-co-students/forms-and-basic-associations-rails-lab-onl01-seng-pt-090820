<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %> <br>

 
  <%= f.label :Artist %><br>
  <%= f.text_field :artist_name, list: "artist_autocomplete" %><br>
    <datalist id="artist_autocomplete">
    <% Artist.all.each do |artist| %>
      <option value="<%= artist.name %>"><br>
    <% end %>
  </datalist>

  <%= f.label :genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>

  <label for="song_notes_1">
  <input id="song_notes_1" name="song[note_contents][]"><br>
  <label for="song_notes_2">
  <input id="song_notes_2" name="song[note_contents][]"><br>

  <%= f.submit %>
<% end %>

